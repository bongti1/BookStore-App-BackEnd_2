{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "builds": [
      {
        "src": "index.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.mjs",
        "methods":["GET","POST","PUT","DELETE"],
        "headers":{
          "Access-Controll-Allow-Origin":"*"
        }
      }
    ]
  }